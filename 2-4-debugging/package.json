{
  "name": "2-4-debugging",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./lib:$NODE_PATH node ./bin/www",
    "docker:build": "docker build -t d4js-2-4 .",
    "docker:build:dev": "docker build -t d4js-2-4:dev -f dev.dockerfile .",
    "docker:run": "npm run docker:stop && npm run docker:rmc && docker run -d -p 3000:3000 --name d4js-2-4 -v d4js2-3:/var/app/node_modules d4js-2-3",
    "docker:run:dev": "npm run docker:stop:dev && npm run docker:rmc:dev && docker run -d -p 3000:3000 --name d4js-2-4-dev -v $PWD:/var/app -v d4js2-3-dev:/var/app/node_modules d4js-2-3:dev",
    "docker:start": "docker start d4js-2-4",
    "docker:start:dev": "docker start d4js-2-4-dev",
    "docker:stop": "docker stop d4js-2-4; exit 0",
    "docker:stop:dev": "docker stop d4js-2-4-dev; exit 0",
    "docker:rmc": "docker rm d4js-2-4; exit 0",
    "docker:rmc:dev": "docker rm d4js-2-4-dev; exit 0"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "epa": "0.2.1",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mongoose": "4.13.2",
    "morgan": "~1.6.1",
    "nanit": "0.3.1",
    "serve-favicon": "~2.3.0"
  }
}
